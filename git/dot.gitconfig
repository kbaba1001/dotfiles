[user]
  name  = kbaba1001
  email = kbaba1001@gmail.com
[color]
  ui   = true
  diff = true
[core]
  editor       = vim
  excludesfile = ~/.gitignore
  quotepath    = false
[push]
  default = simple
[pull]
  rebase = true
[log]
  date = iso
[pager]
  log  = /usr/share/doc/git/contrib/diff-highlight/diff-highlight | less -XF
  show = /usr/share/doc/git/contrib/diff-highlight/diff-highlight | less -XF
  diff = /usr/share/doc/git/contrib/diff-highlight/diff-highlight | less -XF
[gui]
  encoding = utf-8
[help]
  autocorrect = 1
[instaweb]
  local   = true
  httpd   = webrick
  browser = google-chrome
[alias]
  a     = add
  al    = add -A .
  ap    = add -p
  au    = add -u
  b     = branch
  ba    = branch -a
  bm    = branch --merged
  c     = commit -v
  ca    = commit --amend -v
  cb    = checkout -b
  cl    = clone
  cm    = commit -m
  cme   = commit --allow-empty -m
  co    = checkout
  d     = diff
  diary = !sh -c 'git commit -m \"diary $(date --rfc-3339=second)\"'
  dm    = diff master
  dms   = diff master --stat
  ds    = diff --staged
  dw    = diff -w
  f     = fetch
  g     = grep
  l     = !sh -c 'git --no-pager log --oneline --decorate -n 15'
  ld    = log --decorate
  lo    = log --oneline --decorate --graph
  lp    = log -p --decorate
  ls    = log --stat --decorate
  nipo  = !sh -c 'git commit -m \"[kbaba1001] NoMethodError: $(date +%Y-%m-%d)\"'
  p     = pull
  re    = rebase
  ri    = !sh -c 'git rebase -i HEAD~$0'
  ru    = remote update --prune
  s     = show
  si    = !sh -c 'git show HEAD~$(($0 - 1))'
  ss    = show --stat
  t     = tag
