clear Control
clear Lock
clear Shift
clear Mod4
clear Mod5

! Tab      ( 23) => Control_L
! ! c        ( 54) => Control_L
! CapsLock ( 66) => Super_L
! v        ( 55) => Shift_L
! space    ( 34) => Mode_switch
! ! .        ( 60) => Ctrl_R
! ]        ( 35) => Ctrl_R
! m        ( 58) => Shift_R
! Enter    ( 36) => Shift_Lock
keycode 23 = Control_L
! keycode 54 = Control_L
keycode 66 = Super_L
keycode 55 = Shift_L
keycode 34 = Mode_switch
! keycode 60 = Control_R
keycode 35 = Control_R
keycode 58 = Shift_R
keycode 36 = Shift_Lock

add Control = Control_L Control_R
add Mod4    = Super_L
add Shift   = Shift_L Shift_R
add Mod5    = Mode_switch
add Lock    = Shift_Lock

